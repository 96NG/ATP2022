def inicDiferente(s1, s2):
    res = 0
    for i in s1:
        if i not in s2:
            res = res +1
    return res
    
    def acimaMedia(n):
    res = 0
    soma = 0
    for i in range (0, n+1):
        soma += i
        media = soma//n
    for i in range(0, n+1):
        if i > media:
            res += 1
    return res

acimaMedia(5)

def merge(l1, l2):
        novalista = []
        for i in l1:
                novalista.append(i)
        for j in l2:
                novalista.append(j)
        trocas = True
        while trocas:
                trocas = False
                i = 1
                while i < len(novalista):
                        if novalista[i] < novalista[i-1]:
                                novalista [i], novalista[i-1] = novalista[i-1], novalista[i]
                                trocas = True
                        i = i+1
                
        return novalista
